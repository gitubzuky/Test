<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.test">

    <!-- Include following permission if you load images from Internet -->
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- Include following permission if you want to cache images on SD card -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TestActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ConstraintTestActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".WidgetActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".MessageCenterActivity" />
        <activity
            android:name=".widget.picturecheck.BigPictureActivity"
            android:theme="@style/Transparent" />
        <activity android:name=".temp.bigpicture.BigActivity" />
        <activity android:name=".temp.bigpicture.ImagePagerActivity" />
        <activity android:name=".CalActivity"></activity>
    </application>

</manifest>